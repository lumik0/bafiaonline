{
  "dependencies": {
    "@types/bun": "^1.3.5",
    "esbuild": "^0.27.2"
  },
  "scripts": {
    "start": "bun .",
    "a": "esbuild game/src/index.ts --bundle --minify --outfile=game/image/main.js",
    "b": "esbuild launcher/src/index.ts --bundle --serve=80 --servedir=run --watch --outfile=run/index.js",
    "c": "esbuild game/src/index.ts --bundle --minify --outfile=game/image/main.js && bun run build",
    "e": "esbuild launcher/src/index.ts --bundle --outfile=run/index.js",
    "cb": "bun run c && bun run b",
    "build": "bun build/generateImage"
  }
}